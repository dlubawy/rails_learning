<h1><%= @user.name %></h1>
<h2>Created Events</h2>
<% @user.created_events.each do |event| %>
        <h3><%= event.name %></h3> 
        <h4>Date:</h4>
        <%= event.date %>
        <h4>Location:</h4>
        <%= event.location %>
<% end %>

<h2>Upcoming Events</h2>
<% @user.upcoming_events.each do |event| %>
        <h3><%= event.name %></h3>
<% end %>
<h2>Attended Events</h2>
<% @user.previous_events.each do |event| %>
        <h3><%= event.name %></h3>
<% end %>
